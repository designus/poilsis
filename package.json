  "description": "PoilsisPajuryje SPA",
  "main": "index.js",
  "scripts": {
    "analyze": "source-map-explorer ./build/client/main.js{,.map}",
    "clean": "rimraf build",
    "nodemon": "nodemon ./build/server/main.js",
    "start": "cross-env NODE_ENV=production node ./build/server/main.js",
    "start-prod": "npm run build-prod && cross-env NODE_ENV=production node ./build/server/main.js",
    "watch:dev-server": "cd ./src/server && webpack --watch --mode=development",
    "watch:dev-client": "cd ./src/client && webpack-dev-server -w --env dev --disable-host-check",
    "watch-dev": "npm-run-all -p nodemon watch:dev-server watch:dev-client",
    "start:dev-client": "npm run watch:dev-client",
    "start:dev-server": "npm run build:dev-server && cross-env NODE_ENV=development node ./build/server/main.js",
    "start-dev": "cross-env NODE_ENV=development npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run start:dev-server\" \"npm run start:dev-client\"",
    "build:dev-server": "cd ./src/server && webpack --mode=development",
    "build:prod-server": "cd ./src/server && cross-env NODE_ENV=production webpack --mode=production",
    "build:prod-client": "cd ./src/client && cross-env NODE_ENV=production webpack --env prod",
    "build-prod": "npm run clean && npm-run-all build:prod-server build:prod-client",
    "lint": "tslint --fix \"src/**/**.ts*\"",
    "test": "jest --env=node --forceExit --runInBand"
  },
