  "description": "PoilsisPajuryje SPA",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "cross-env NODE_ENV=production node ./build/server/app.js",
    "build-client": "cd ./src/client && cross-env NODE_ENV=production webpack --env prod",
    "dev-client": "cd ./src/client && webpack-dev-server -w --env dev",
    "build-server": "cd ./src/server && cross-env NODE_ENV=production webpack --env prod",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node ./build/server/server/server.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run build-server\" \"npm run build-client\"",
    "lint": "tslint \"src/**/**.ts*\""
  },
